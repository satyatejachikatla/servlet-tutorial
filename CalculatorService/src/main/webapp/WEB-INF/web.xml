<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">



	<servlet>
		<servlet-name>PhoneDetailService</servlet-name>
		<servlet-class>com.satya.CalculatorService.PhoneDetailService</servlet-class>
		<init-param>
			<param-name>Name</param-name>
			<param-value>Teja</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>PhoneDetailService</servlet-name>
		<url-pattern>/phone</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>Phone</param-name>
		<param-value>Redmi</param-value>
	</context-param>


</web-app>